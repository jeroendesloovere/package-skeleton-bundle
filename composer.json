{
    "name": "jeroendesloovere/skeleton-bundle",
    "type": "library",
    "description": "This Symfony 2 bundle is a skeleton used for fast bundle generating to load in a class as a service.",
    "keywords": ["symfony2", "bundle", "php"],
    "homepage": "https://github.com/jeroendesloovere/skeleton-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Desloovere",
            "email": "info@jeroendesloovere.be",
            "homepage": "http://jeroendesloovere.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/http-kernel": "~2.3",
        "jeroendesloovere/skeleton": "1.0.*"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.3"
    },
    "autoload": {
        "psr-4": { "JeroenDesloovere\\Bundle\\SkeletonBundle\\": "" }
    }
}